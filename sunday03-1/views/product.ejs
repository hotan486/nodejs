<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>/views/porduct.ejs</title>
</head>
<body>
    
    <h2>자동차 목록 페이지</h2>
    <%
        //console.log('자동차 목록 ejs 페이지');
        //console.dir(carList);
    %>
    
    <table border="1">
        <tr>
            <th>ObjectId</th>
            <th>Name</th>
            <th>Price</th>
            <th>Company</th>
            <th>Year</th>
        </tr>
        <%
        for(var i=0; i<carList.length; i++) { 
            var car = carList[i];
            console.dir(car);
        %>
        <tr>
            <td><%=car._id%></td>
            <td><a href="/process/product/<%=car._id%>"><%=car.name%></a></td>
            <td><%=car.price%></td>
            <td><%=car.company%></td>
            <td><%=car.year%></td>
        </tr>
        <% } %>
    </table>
</body>
</html>